//output:index.html

doctype html
head
  meta(charset="utf-8")
  meta(name='viewport', content='width=device-width, initial-scale=1')
  link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css")
  link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css")
  link(rel="stylesheet", href="../css/index.css")
  title YR Fang | Front-End Developer
body
  .menu#side-nav
    nav#menu_toggle
      i#toggle.fa.fa-bars
    ul
      li
        a(href="#section_about")
          .title ABOUT
          .infoIcon
            .fa.fa-flask
          p Hello
      li
        a(href="#")
          .title WORK
          .infoIcon
            .fa.fa-pencil
          p Case Studies?
      li
        a(href="#")
          .title CONTACT
          .infoIcon
            .fa.fa-envelope
          p Let's get in touch
  #content
    section#section_about
      .outerContainer
        .row
          .screenCover
            .coverPhoto
            .coverLowerPhoto
          .col-xs-12.heading
            h2.title Ying Ru Fang
            h6.subtitle Front-end Developer | Taiwan, Taipei <br> Create something powerful | change the people's mind
          .col-xs-12.col-lg-4
            .row
              .col-xs-6
                .iconContainer.left
                  img.imgIcon(src="https://image.flaticon.com/icons/svg/85/85047.svg")
                  h4.title Resume
                  h6.sub__title View resume
              .col-xs-6
                .iconContainer.right
                  img.imgIcon(src="https://image.flaticon.com/icons/svg/24/24107.svg")
                  h4.title GitHub
                  h6.sub__title View Repositories
    
            


  script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js")
  script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js")
  script(src="https://unpkg.com/vue/dist/vue.js")
  script(src="../index.js")
  
